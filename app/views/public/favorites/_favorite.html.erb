<% if post.favorited_by?(current_end_user) %>
  <%= link_to post_favorite_path(post), method: :delete, remote: true, date: {"turbolinks" => false} do %>
    ♥︎<%= post.favorites.count %>いいね
  <% end %>
<% else %>
  <%= link_to post_favorite_path(post), method: :post, remote: true, date: {"turbolinks" => false} do %>
    ♡<%= post.favorites.count %>いいね
  <% end %>
<% end %>